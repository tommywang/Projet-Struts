<?xml version="1.0" encoding="ASCII"?>
<mvcmetamodel:Application xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mvcmetamodel="http://mvcmetamodel/1.0" name="MyApp">
  <webxml name="MVCStruts">
    <resourceRef resRefName="jdbc/dsMyDB" description="JDBC resource to access my DB i.e. dfj_hibernate" resType="javax.sql.DataSource" resAuth="Container"/>
    <servlet name="action" class="org.apache.struts.action.ActionServlet" loadOnStartup="1">
      <initParam name="config" value="struts-config.xml"/>
    </servlet>
    <welcomeFileList>
      <welcomeFile pageName="//@page.0"/>
    </welcomeFileList>
  </webxml>
  <resources name="Resources.properties"/>
  <page name="main" type="jsp" title="main.page.title">
    <component xsi:type="mvcmetamodel:Link" url="ContactCreation.do" text="main.addcontact.link"/>
  </page>
  <page name="contactCreation" type="jsp" title="add.contact"/>
</mvcmetamodel:Application>
